<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="renderer" content="webkit">
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <title>沃港后台管理系统</title>
    <link rel="stylesheet" href="~/Content/Css/amazeui.min.css" />
    <link rel="stylesheet" href="~/Content/Css/amazeui.datatables.min.css" />
    <link rel="stylesheet" href="~/Content/Css/app.css">
    <link rel="stylesheet" href="~/Content/Css/layer.css">
    <link rel="stylesheet" href="~/Content/Css/webuploader.css">
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/layer.js"></script>
</head>
<body  data-type="index">
    <script src="~/scripts/theme.js"></script>
    <div class="am-g tpl-g">
        <!-- 头部 -->
        @Html.Action("PartialHead", "Partial")  
        
        <!-- 内容区域 -->
        <div class="tpl-content-wrapper">


            <div class="row-content am-cf">
                @RenderBody()
                </div>
            </div>
        </div>
        <script src="http://cdn.bootcss.com/amazeui/2.7.2/js/amazeui.min.js"></script>
        <script src="~/scripts/webuploader.min.js"></script>
        <script src="~/scripts/amazeui.datatables.min.js"></script>
        <script src="~/scripts/dataTables.responsive.min.js"></script>
        <script src="~/scripts/app.js"></script>
        <script type="text/javascript">
             @RenderSection("scripts", false)
        </script>
</body>
</html>